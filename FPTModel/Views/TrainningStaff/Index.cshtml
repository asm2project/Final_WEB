        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.Fullname)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Cours.CouseName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User.Fullname)
        </td>
